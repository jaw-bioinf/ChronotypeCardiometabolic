	number = {5},
	journal = {International journal of obesity (2005)},
	author = {Harvie, M N and Pegington, M and Mattson, M P and Frystyk, J and Dillon, B and Evans, G and Cuzick, J and Jebb, S A and Martin, B and Cutler, R G and Son, T G and Maudsley, S and Carlson, O D and Egan, J M and Flyvbjerg, A and Howell, A},
	month = may,
	year = {2011},
	pmid = {20921964},
	keywords = {Adult, Biological Markers, Breast Neoplasms, Caloric Restriction, Cardiovascular Diseases, Feasibility Studies, Female, Humans, Insulin Resistance, Metabolic Syndrome X, Middle Aged, Overweight, Patient Compliance, Risk Factors, Weight Loss},
