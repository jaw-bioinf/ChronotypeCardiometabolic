	keywords = {Algorithms, Archaeal Proteins, Bacteriophage lambda, Computational Biology, Halobacterium, Internet, Models, Biological, Neural Networks, Computer, Phenotype, Software, Software Design, Stochastic Processes},
	pages = {2498--2504},
}

