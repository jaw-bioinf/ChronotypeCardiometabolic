	pmid = {21572417},
	pmcid = {PMC3115696},
	keywords = {Adult, Child, Child Development Disorders, Pervasive, Exons, Female, Genetic Predisposition to Disease, Humans, Male, Middle Aged, Mutation, Pedigree, Sequence Analysis, DNA},
