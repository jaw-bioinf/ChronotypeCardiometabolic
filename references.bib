	pmid = {18854154},
	pmcid = {PMC2628946},
	keywords = {Cell Line, HIV Infections, HIV-1, Host-Pathogen Interactions, Humans, Proteins, RNA Interference, Two-Hybrid System Techniques, Virus Replication},
	pages = {49--60},
}

@article{hancock_mouse_2009,
