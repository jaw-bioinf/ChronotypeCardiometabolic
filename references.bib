	keywords = {Cyclopentanes, Enzyme Inhibitors, Genome, Human, Humans, Proteome, Pyrimidines, Ubiquitin-Protein Ligases, Ubiquitination},
	pages = {459--474},
}

@article{strausberg_generation_2002,
	title = {Generation and initial analysis of more than 15,000 full-length human and mouse {cDNA} sequences},
	volume = {99},
