	urldate = {2018-02-06},
	journal = {JMIR Medical Informatics},
	author = {Beyan, Timur and Aydın Son, Yeşim},
	month = aug,
	year = {2014},
	pmid = {25599817},
	pmcid = {PMC4288069},
}

