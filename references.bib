@article{wells_anatomical_2015-1,
	title = {The anatomical distribution of genetic associations},
	volume = {43},
	issn = {1362-4962},
	doi = {10.1093/nar/gkv1262},
	abstract = {Deeper understanding of the anatomical intermediaries for disease and other complex genetic traits is essential to understanding mechanisms and developing new interventions. Existing ontology tools provide functional, curated annotations for many genes and can be used to develop mechanistic hypotheses; yet information about the spatial expression of genes may be equally useful in interpreting results and forming novel hypotheses for a trait. Therefore, we developed an approach for statistically testing the relationship between gene expression across the body and sets of candidate genes from across the genome. We validated this tool and tested its utility on three applications. First, we show that the expression of genes in associated loci from GWA studies implicates specific tissues for 57 out of 98 traits. Second, we tested the ability of the tool to identify novel relationships between gene expression and phenotypes. Specifically, we experimentally confirmed an underappreciated prediction highlighted by our tool: that white blood cell count--a quantitative trait of the immune system--is genetically modulated by genes expressed in the skin. Finally, using gene lists derived from exome sequencing data, we show that human genes under selective constraint are disproportionately expressed in nervous system tissues.},
	language = {eng},
	number = {22},
	journal = {Nucleic Acids Research},
	author = {Wells, Alan and Kopp, Nathan and Xu, Xiaoxiao and O'Brien, David R. and Yang, Wei and Nehorai, Arye and Adair-Kirk, Tracy L. and Kopan, Raphael and Dougherty, J. D.},
	month = dec,
	year = {2015},
	pmid = {26586807},
	pmcid = {PMC4678833},
	keywords = {Algorithms, Animals, Data Interpretation, Statistical, Gene Expression, Genome-Wide Association Study, Genomics, Humans, Leukocytes, Mice, Mice, Transgenic, Nervous System, Organ Specificity, Phenotype, Tissue Distribution, disease},
	pages = {10804--10820},
}

@article{shimoyama_three_2012,
	title = {Three {Ontologies} to {Define} {Phenotype} {Measurement} {Data}},
