	keywords = {Computational Biology, Computer Simulation, Cytoplasm, Internet, Membrane Proteins, Models, Molecular, Models, Theoretical, Programming Languages, Protein Conformation, Protein Sorting Signals, Protein Structure, Secondary, Proteomics, Software},
	pages = {W429--432},
}

