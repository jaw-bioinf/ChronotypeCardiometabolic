	pmid = {22889512},
	keywords = {Adipose Tissue, Adult, Aged, Blood Pressure, Cholesterol, Diet, High-Fat, Diet, Reducing, Fasting, Female, Heart Rate, Humans, Middle Aged, Obesity, Triglycerides, Waist Circumference, Weight Loss},
