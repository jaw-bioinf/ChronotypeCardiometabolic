@incollection{rossetti_chapter_2018,
	series = {The {Parietal} {Lobe}},
	title = {Chapter 11 - {Optic} ataxia: beyond the dorsal stream cliché},
	volume = {151},
	shorttitle = {Chapter 11 - {Optic} ataxia},
	url = {http://www.sciencedirect.com/science/article/pii/B9780444636225000115},
	abstract = {This chapter reviews clinical and scientific approaches to optic ataxia. This double historic track allows us to address important issues such as the link between Bálint syndrome and optic ataxia, the alleged double dissociation between optic ataxia and visual agnosia, and the use of optic ataxia to argue for a specific vision-for-action occipitoposterior parietal stream. Clinical cases are described and reveal that perceptual deficits have been long shown to accompany ataxia. Importantly, the term ataxia appears to be misleading as patients exhibit a combination of visual and nonvisual perceptual, attentional, and visuomotor guidance deficits, which are confirmed by experimental approaches. Three major features of optic ataxia are described. The first is a spatial feature whereby the deficits exhibited by patients appear to be specific to peripheral vision, akin to the field effect. Visuomotor field examination allows us to quantify this deficit and reveals that it consists of a highly reliable retinocentric hypometria. The third is a temporal feature whereby these deficits are exacerbated under temporal constraints, i.e., when attending to dynamic stimuli. These two aspects combine in a situation where patients have to quickly respond to a target presented in peripheral vision that is experimentally displaced upon movement onset. In addition to the field effect, a hand effect can be described in conditions where the hand is not visible. Spatial and temporal aspects as well as field and hand effects may rely on several posterior parietal modules that remain to be precisely identified both anatomically and functionally. It is concluded that optic ataxia is not a visuomotor deficit and there is no dissociation between perception and action capacities in optic ataxia, hence a fortiori no double dissociation between optic ataxia and visual agnosia. Future directions for understanding the basic pathophysiology of optic ataxia are proposed.},
	urldate = {2018-10-19},
	booktitle = {Handbook of {Clinical} {Neurology}},
	publisher = {Elsevier},
	author = {Rossetti, Yves and Pisella, Laure},
	editor = {Vallar, Giuseppe and Coslett, H. Branch},
	month = jan,
	year = {2018},
	doi = {10.1016/B978-0-444-63622-5.00011-5},
	keywords = {Bálint, action, attention, automatic pilot, illusion, motor control, optic ataxia, parietal, peripheral vision, sensorimotor, vision},
	pages = {225--247},
}

