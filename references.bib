}

@article{zhang_genome-wide_2009,
	title = {A {Genome}-{Wide} {RNAi} {Screen} for {Modifiers} of the {Circadian} {Clock} in {Human} {Cells}},
	volume = {139},
	issn = {0092-8674},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2777987/},
	doi = {10.1016/j.cell.2009.08.031},
	abstract = {Two decades of research identified more than a dozen clock genes and defined a biochemical feedback mechanism of circadian oscillator function. To identify additional clock genes and modifiers, we conducted a genome-wide siRNA screen in a human cellular clock model. Knockdown of nearly a thousand genes reduced rhythm amplitude. Potent effects on period length or increased amplitude were less frequent; we found hundreds of these and confirmed them in secondary screens. Characterization of a subset of these genes demonstrated a dosage-dependent effect on oscillator function. Protein interaction network analysis showed that dozens of gene products directly or indirectly associate with known clock components. Pathway analysis revealed these genes are overrepresented for components of insulin and hedgehog signaling, the cell cycle, and the folate metabolism. Coupled with data showing many of these pathways are clock-regulated, we conclude the clock is interconnected with many aspects of cellular function.},
	number = {1},
	urldate = {2017-01-03},
	journal = {Cell},
	author = {Zhang, Eric E. and Liu, Andrew C. and Hirota, Tsuyoshi and Miraglia, Loren J. and Welch, Genevieve and Pongsawakul, Pagkapol Y. and Liu, Xianzhong and Atwood, Ann and Huss, Jon W. and Janes, Jeff and Su, Andrew I. and Hogenesch, John B. and Kay, Steve A.},
	month = oct,
	year = {2009},
	pmid = {19765810},
	pmcid = {PMC2777987},
	pages = {199--210},
}

