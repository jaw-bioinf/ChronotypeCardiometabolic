	number = {5},
	journal = {Bioinformatics (Oxford, England)},
	author = {Yanai, Itai and Benjamin, Hila and Shmoish, Michael and Chalifa-Caspi, Vered and Shklar, Maxim and Ophir, Ron and Bar-Even, Arren and Horn-Saban, Shirley and Safran, Marilyn and Domany, Eytan and Lancet, Doron and Shmueli, Orit},
	month = mar,
	year = {2005},
	pmid = {15388519},
	keywords = {Algorithms, Chromosome Mapping, Gene Expression Profiling, Humans, Oligonucleotide Array Sequence Analysis, Organ Specificity, Proteome, Software, Tissue Distribution, Transcription Factors},
