	keywords = {Actins, Algorithms, Amino Acid Sequence, Animals, Carrier Proteins, Escherichia coli Proteins, Molecular Structure, Myoglobin, Periplasmic Binding Proteins, Protein Conformation, Proteins, Receptors, Cyclic AMP, Structure-Activity Relationship},
	pages = {164--170},
