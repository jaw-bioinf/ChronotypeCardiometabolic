@article{nguyen_dna_2002,
	title = {{DNA} microarray experiments: biological and technological aspects},
	volume = {58},
	issn = {0006-341X},
	shorttitle = {{DNA} microarray experiments},
	abstract = {DNA microarray technologies, such as cDNA and oligonucleotide microarrays, promise to revolutionize biological research and further our understanding of biological processes. Due to the complex nature and sheer amount of data produced from microarray experiments, biologists have sought the collaboration of experts in the analytical sciences, including statisticians, among others. However, the biological and technical intricacies of microarray experiments are not easily accessible to analytical experts. One aim for this review is to provide a bridge to some of the relevant biological and technical aspects involved in microarray experiments. While there is already a large literature on the broad applications of the technology, basic research on the technology itself and studies to understand process variation remain in their infancy. We emphasize the importance of basic research in DNA array technologies to improve the reliability of future experiments.},
