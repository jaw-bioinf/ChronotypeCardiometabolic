	keywords = {Amygdala, Autistic Disorder, Cell Count, Cerebellum, Cerebral Cortex, Child, Child, Preschool, Humans, Infant, Magnetic Resonance Imaging, Neurons, Organ Size},
	pages = {137--145},
}

@article{ha_characteristics_2015,
