	keywords = {Amino Acid Sequence, Data mining, Databases, Protein, EC numbers, Gene Ontology, Molecular Sequence Data, Proteins, Sequence Alignment, Sequence Analysis, Protein, Structure-Activity Relationship, function prediction, homology, orthology, paralogy},
	pages = {4.12.1--8},
}

@article{sunkin_allen_2013,
	title = {Allen {Brain} {Atlas}: an integrated spatio-temporal portal for exploring the central nervous system},
