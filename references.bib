	keywords = {Adolescent, Adult, Body Mass Index, Body Weight, Ceremonial Behavior, Cholesterol, HDL, Cholesterol, LDL, Fasting, Female, Humans, Iran, Islam, Male, Prospective Studies, Triglycerides},
