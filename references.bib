	keywords = {Actigraphy, Autism Spectrum Disorder, Child, Child, Preschool, Female, Health Education, Humans, Male, Parents, Sleep, Sleep Initiation and Maintenance Disorders, Sleep Medicine Specialty, Sleep Wake Disorders, Surveys and Questionnaires},
	pages = {665--676},
}

@article{klei_common_2012,
	title = {Common genetic variants, acting additively, are a major source of risk for autism},
