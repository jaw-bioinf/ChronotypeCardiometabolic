	keywords = {Biomarkers, Tumor, Breast Neoplasms, Female, Gene Expression Profiling, Humans, Molecular Targeted Therapy, Mutation, Precision Medicine, Predictive Value of Tests, Proto-Oncogene Proteins B-raf},
	pages = {15},
}

@article{schilsky_personalized_2010,
	title = {Personalized medicine in oncology: the future is now},
	volume = {9},
