	pmid = {23401553},
	keywords = {Adipogenesis, Animals, Gene Expression Profiling, Gene Expression Regulation, Gene Knockdown Techniques, Information Theory, Male, Mice, Oligonucleotide Array Sequence Analysis, Open Reading Frames, Phenotype, RNA, Long Noncoding, Reproducibility of Results, Transcriptome},
	pages = {3387--3392},
