	journal = {Cancer Genetics},
	author = {Li, Marilyn M. and Monzon, Federico A. and Biegel, Jaclyn A. and Jobanputra, Vaidehi and Laffin, Jennifer J. and Levy, Brynn and Leon, Annette and Miron, Patricia and Rossi, Michael R. and Toruner, Gokce and Alvarez, Karla and Doho, Gregory and Dougherty, Margaret J. and Hu, Xiaofeng and Kash, Shera and Streck, Deanna and Znoyko, Iya and Hagenkord, Jill M. and Wolff, Daynna J.},
