	keywords = {Animals, Biological Clocks, Blotting, Northern, Cell Culture Techniques, Cell Line, Cells, Cultured, Circadian Rhythm, Diabetes Mellitus, Experimental, Gene Expression, Gene Expression Profiling, Hepatocytes, Hypoglycemic Agents, Insulin, Liver, Luciferases, Male, Oligonucleotide Array Sequence Analysis, Period Circadian Proteins, Rats, Rats, Transgenic, Rats, Wistar},
