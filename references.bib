	keywords = {Body Weight, C-Reactive Protein, Caloric Restriction, Calorie restriction, Cholesterol, Coronary heart disease, Humans, Leptin, Liquid diet, Obese women, Visceral fat, intermittent fasting, markers, mechanisms, metabolic   syndrome, system, weight-loss},
