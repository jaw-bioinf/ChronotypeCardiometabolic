	pmid = {15591335},
	keywords = {Algorithms, Antineoplastic Agents, Hormonal, Biomarkers, Tumor, Breast Neoplasms, DNA, Neoplasm, Estrogen Antagonists, Female, Follow-Up Studies, Gene Expression, Genes, erbB-2, Humans, Lymphatic Metastasis, Middle Aged, Multivariate Analysis, Neoplasm Metastasis, Prognosis, Proportional Hazards Models, Receptors, Estrogen, Receptors, Progesterone, Recurrence, Reverse Transcriptase Polymerase Chain Reaction, Risk, Survival Analysis, Tamoxifen},
