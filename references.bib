	pmid = {22495306},
	pmcid = {PMC3667984},
	keywords = {Alleles, Autistic Disorder, Codon, Nonsense, Exome, Exons, Genetic Heterogeneity, Genetic Predisposition to Disease, Humans, Mutation, NAV1.2 Voltage-Gated Sodium Channel, Nerve Tissue Proteins, RNA Splice Sites, Siblings, Sodium Channels},
