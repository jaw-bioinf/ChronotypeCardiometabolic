	journal = {EBioMedicine},
	author = {Moon, Joung-Ho and Cho, Chul-Hyun and Son, Gi Hoon and Geum, Dongho and Chung, Sooyoung and Kim, Hyun and Kang, Seung-Gul and Park, Young-Min and Yoon, Ho-Kyoung and Kim, Leen and Jee, Hee-Jung and An, Hyonggin and Kripke, Daniel F. and Lee, Heon-Jeong},
	month = sep,
