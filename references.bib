	author = {Wiggins, Lisa D. and Tian, Lin H. and Levy, Susan E. and Rice, Catherine and Lee, Li-Ching and Schieve, Laura and Pandey, Juhi and Daniels, Julie and Blaskey, Lisa and Hepburn, Susan and Landa, Rebecca and Edmondson-Pretzel, Rebecca and Thompson, William},
	month = nov,
