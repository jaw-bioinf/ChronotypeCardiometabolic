	keywords = {Blood Pressure, Caloric Restriction, Cholesterol, Gluconeogenesis, Humans, Rats, blood-pressure, breakfast, cholesterol   metabolism, controlled diet, fed controlled diets, intermittent fasting, lipid profiles, meal frequency, normal-weight adults, plasma, risk-factors},
