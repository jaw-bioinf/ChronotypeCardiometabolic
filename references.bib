}

@article{atwood_cell-autonomous_2011,
	title = {Cell-autonomous circadian clock of hepatocytes drives rhythms in transcription and polyamine synthesis},
	volume = {108},
	issn = {1091-6490},
	doi = {10.1073/pnas.1115753108},
	abstract = {The circadian clock generates daily rhythms in mammalian liver processes, such as glucose and lipid homeostasis, xenobiotic metabolism, and regeneration. The mechanisms governing these rhythms are not well understood, particularly the distinct contributions of the cell-autonomous clock and central pacemaker to rhythmic liver physiology. Through microarray expression profiling in Met murine hepatocytes (MMH)-D3, we identified over 1,000 transcripts that exhibit circadian oscillations, demonstrating that the cell-autonomous clock can drive many rhythms, and that MMH-D3 is a valid circadian model system. The genes represented by these circadian transcripts displayed both cophasic and antiphasic organization within a protein-protein interaction network, suggesting the existence of competition for binding sites or partners by genes of disparate transcriptional phases. Multiple pathways displayed enrichment in MMH-D3 circadian transcripts, including the polyamine synthesis module of the glutathione metabolic pathway. The polyamine synthesis module, which is highly associated with cell proliferation and whose products are required for initiation of liver regeneration, includes enzymes whose transcripts exhibit circadian oscillations, such as ornithine decarboxylase and spermidine synthase. Metabolic profiling revealed that the enzymatic product of spermidine synthase, spermidine, cycles as well. Thus, the cell-autonomous hepatocyte clock can drive a significant amount of transcriptional rhythms and orchestrate physiologically relevant modules such as polyamine synthesis.},
	language = {eng},
	number = {45},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Atwood, Ann and DeConde, Robert and Wang, Susanna S. and Mockler, Todd C. and Sabir, Jamal S. M. and Ideker, Trey and Kay, Steve A.},
	month = nov,
	year = {2011},
	pmid = {22042857},
	pmcid = {PMC3215069},
	keywords = {Animals, Biogenic Polyamines, Circadian Rhythm, Hepatocytes, Mice, Transcription, Genetic},
	pages = {18560--18565},
}

@article{pembroke_temporal_2015-1,
	title = {Temporal transcriptomics suggest that twin-peaking genes reset the clock},
	volume = {4},
	issn = {2050-084X},
	doi = {10.7554/eLife.10518},
	abstract = {The mammalian suprachiasmatic nucleus (SCN) drives daily rhythmic behavior and physiology, yet a detailed understanding of its coordinated transcriptional programmes is lacking. To reveal the finer details of circadian variation in the mammalian SCN transcriptome we combined laser-capture microdissection and RNA-seq over a 24-hour light/dark cycle. We show that 7-times more genes exhibited a classic sinusoidal expression signature than previously observed in the SCN. Another group of 766 genes unexpectedly peaked twice, near both the start and end of the dark phase; this twin-peaking group is significantly enriched for synaptic transmission genes that are crucial for light-induced phase shifting of the circadian clock. 341 intergenic non-coding RNAs, together with novel exons of annotated protein-coding genes, including Cry1, also show specific circadian expression variation. Overall, our data provide an important chronobiological resource (www.wgpembroke.com/shiny/SCNseq/) and allow us to propose that transcriptional timing in the SCN is gating clock resetting mechanisms.},
	language = {ENG},
	journal = {eLife},
	author = {Pembroke, William G. and Babbs, Arran and Davies, Kay and Ponting, Chris P. and Oliver, Peter L.},
	month = nov,
	year = {2015},
	pmid = {26523393},
	keywords = {Evolutionary Biology, Genomics, mouse, neuroscience},
}

