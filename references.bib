@article{sun_discovery_2015,
	title = {Discovery, {Annotation}, and {Functional} {Analysis} of {Long} {Noncoding} {RNAs} {Controlling} {Cell}-{Cycle} {Gene} {Expression} and {Proliferation} in {Breast} {Cancer} {Cells}},
