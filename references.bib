@article{mccarthy_genome-wide_2008,
	title = {Genome-wide association studies for complex traits: consensus, uncertainty and challenges},
	volume = {9},
	issn = {1471-0064},
	shorttitle = {Genome-wide association studies for complex traits},
	doi = {10.1038/nrg2344},
