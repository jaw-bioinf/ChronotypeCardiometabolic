@article{brown_telling_2019,
	title = {Telling the {Time} with a {Broken} {Clock}: {Quantifying} {Circadian} {Disruption} in {Animal} {Models}},
	volume = {8},
	issn = {2079-7737},
	shorttitle = {Telling the {Time} with a {Broken} {Clock}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6466320/},
	doi = {10.3390/biology8010018},
	abstract = {Circadian rhythms are approximately 24 h cycles in physiology and behaviour that enable organisms to anticipate predictable rhythmic changes in their environment. These rhythms are a hallmark of normal healthy physiology, and disruption of circadian rhythms has implications for cognitive, metabolic, cardiovascular and immune function. Circadian disruption is of increasing concern, and may occur as a result of the pressures of our modern 24/7 society—including artificial light exposure, shift-work and jet-lag. In addition, circadian disruption is a common comorbidity in many different conditions, ranging from aging to neurological disorders. A key feature of circadian disruption is the breakdown of robust, reproducible rhythms with increasing fragmentation between activity and rest. Circadian researchers have developed a range of methods for estimating the period of time series, typically based upon periodogram analysis. However, the methods used to quantify circadian disruption across the literature are not consistent. Here we describe a range of different measures that have been used to measure circadian disruption, with a particular focus on laboratory rodent data. These methods include periodogram power, variability in activity onset, light phase activity, activity bouts, interdaily stability, intradaily variability and relative amplitude. The strengths and limitations of these methods are described, as well as their normal ranges and interrelationships. Whilst there is an increasing appreciation of circadian disruption as both a risk to health and a potential therapeutic target, greater consistency in the quantification of disrupted rhythms is needed.},
	number = {1},
	urldate = {2020-03-05},
	journal = {Biology},
	author = {Brown, Laurence A. and Fisk, Angus S. and Pothecary, Carina A. and Peirson, Stuart N.},
	month = mar,
	year = {2019},
	pmid = {30901884},
	pmcid = {PMC6466320},
}

